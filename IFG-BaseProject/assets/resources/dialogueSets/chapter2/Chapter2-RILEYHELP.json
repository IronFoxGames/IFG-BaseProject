    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "You're friends with Clyde Townsend, right?",
          "characterSprites": [
            "Thinking"
          ],
