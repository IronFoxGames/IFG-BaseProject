  "dialogueSections": [
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Hey... Uncle Joey?",
          "characterSprites": [
            "Thinking"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "What is it sweetheart?",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Did Darcy seem kinda... off to you? Like mad at me?",
          "characterSprites": [
            "Thinking"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I think you just caught her off guard. I'm surprised you didn't tell her you were coming home.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I suppose... It was all so rushed. I didn't even think of it.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "There was a time that wouldn't have been a question. You two were so in sync you'd finish each other sentences.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "You think she's worried we've lost that? I didn't realise.",
          "characterSprites": [
            "Thinking"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Well the fact you have to ask is pretty telling. You've been gone a long time Viola, your mind has been on other things.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        },
        {
          "line": "We've all missed you... Oh, customer, gotta run. Morning Clyde! Your usual tables open.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
