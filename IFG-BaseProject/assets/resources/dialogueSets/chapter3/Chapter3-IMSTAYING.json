  "dialogueSections": [
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Darcy! I've got something to tell you!",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Darcy"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Woah, you seem really excited. Sit Down and spill the tea.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I quit my job! I was talking to my boss and he was playing hardball... And then I realized that job wasn't what I really wanted.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        },
        {
          "line": "So I told him they need me a lot more than I need them, and that I'm staying here!",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Darcy"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "For real, Viola? That's amazing!",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Ellie"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Aunty Viola is going to stay? Can we play more?",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "That's right! I'm staying right here, and we can play as much as you want!",
          "characterSprites": [
            "Surprised"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Ellie"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Yayyyy! Can you teach me that card game you're always playing with Joey?",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Darcy"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "We're so happy, you're staying!",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I'm feeling lighter than air! I'd better go tell everyone else.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    }
