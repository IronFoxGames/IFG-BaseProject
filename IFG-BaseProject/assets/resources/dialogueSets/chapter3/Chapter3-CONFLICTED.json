    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Will? You startled me.",
          "characterSprites": [
            "Surprised"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
