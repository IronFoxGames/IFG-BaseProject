{
  "dialogueSections": [
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I don't know what to say!",
          "characterSprites": [
            "Surprised"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I've done all the paperwork. The diner officially belongs to you. I didn't forget you either Chef. You have 10% ownership too.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Chef"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Woah, buddy. I can't believe you did that for me!",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "I want you to treat this place as your own. Don't be afraid to make changes. You've already shown you have great taste.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Thank you Uncle Joey! I promise I'll take good care of the Diner. And Chef.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Chef"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Well... at least take good care of the diner.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "And you had better treat Viola better than you did me.",
          "characterSprites": [
            "Neutral"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Chef"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Aw c'mon Joe. You know we had a good time. I'll miss having you around.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 1,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Haha, we always did! Don't worry buddy, I'll be checking in on you both.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Viola"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Wow... This is the beginning of a new chapter for all of us.",
          "characterSprites": [
            "Happy"
          ],
          "unskippable": false
        }
      ]
    }
  ],
