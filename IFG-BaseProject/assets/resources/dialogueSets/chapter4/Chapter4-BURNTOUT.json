    {
      "characterSide": 1,
      "characterNames": [
        "Chef"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "How am I supposed to keep working in these conditions?",
          "characterSprites": [
            "Angry"
          ],
          "unskippable": false
        }
      ]
    },
    {
      "characterSide": 0,
      "characterNames": [
        "Joey"
      ],
      "dialogueType": "Speaking",
      "lines": [
        {
          "line": "Ah, quit yer yappin', you've always got something to complain about!",
